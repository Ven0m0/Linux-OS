### General ###
silent
show-error
# follow redirects
location
# Wait before timing out.
connect-timeout 15
create-dirs
fail-early
fail
netrc-optional

# HTTP
tls-max 1.3
http3
tcp-fastopen
tcp-nodelay
tls-earlydata
mptcp
ssl-no-revoke
# ipv4

# Compression
compressed
compressed-ssh
tr-encoding
parallel
parallel-max 16

# When following a redirect, automatically set the previous URL as referer.
referer ";auto"



# Use the server-provided timestamp, if any.
remote-time
remote-name-all
# remote-name
