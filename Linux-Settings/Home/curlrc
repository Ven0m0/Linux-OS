# Use strongest modern TLS only
--tlsv1.2
--tlsv1.3

# HTTP versions
#--http2
--http3

# Compression & HSTS
--compressed
--compressed-ssh
# --hsts

# Parallel
-Z

# Network performance
--tcp-fastopen
--tcp-nodelay

# Create dirs for output
--create-dirs

# Not always supported
--tls-earlydata
--mptcp

# When following a redirect, automatically set the previous URL as referer.
referer = ";auto"

# Wait 60 seconds before timing out.
connect-timeout = 60
